{
    "env": {
        "node": true,
        "browser": true,
        "es6": true 
    }, 
    "parser": "babel-eslint", 
    "extends": "airbnb", 
    "rules": { 
        "react/jsx-filename-extension": [ 
            1, 
            { 
                "extensions": [ ".js", ".jsx" ] 
            }
        ],
        "prettier/prettier": [
            "error",
            {
              "trailingComma": "es5",
              "singleQuote": true,
              "printWidth": 100
            }
        ],
        "import/no-named-as-default": 0
    },
    "plugins": [
        "prettier"
    ]
}